<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.LoginController">
   <children>
      <AnchorPane layoutX="85.0" layoutY="111.0" prefHeight="517.0" prefWidth="630.0" style="-fx-background-color: grey;">
         <children>
            <Label layoutX="228.0" layoutY="40.0" prefHeight="64.0" prefWidth="175.0" text="Login" textFill="WHITE">
               <font>
                  <Font size="51.0" />
               </font>
            </Label>
            <TextField fx:id="userName" layoutX="232.0" layoutY="174.0" prefHeight="27.0" prefWidth="220.0" />
            <Label layoutX="101.0" layoutY="166.0" prefHeight="44.0" prefWidth="169.0" text="UserName:" textFill="#f5f0f0">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <Label layoutX="101.0" layoutY="215.0" prefHeight="44.0" prefWidth="137.0" text="Password:" textFill="#f5f0f0">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <PasswordField fx:id="pass" layoutX="232.0" layoutY="224.0" prefHeight="27.0" prefWidth="220.0" />
            <Button fx:id="submit" layoutX="256.0" layoutY="280.0" mnemonicParsing="false" onAction="#handle" prefHeight="44.0" prefWidth="137.0" text="Submit" textFill="#141414">
               <font>
                  <Font size="23.0" />
               </font>
            </Button>
            <AnchorPane layoutX="166.0" layoutY="346.0" prefHeight="93.0" prefWidth="275.0" style="-fx-background-color: #EDEDED;">
               <children>
                  <Label layoutX="40.0" prefHeight="44.0" prefWidth="195.0" text="Dont't have the Account?">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <Button fx:id="register" layoutX="87.0" layoutY="44.0" mnemonicParsing="false" onAction="#click" text="Register here" />
               </children>
            </AnchorPane>
            <Label fx:id="message" layoutX="121.0" layoutY="129.0" prefHeight="34.0" prefWidth="337.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
